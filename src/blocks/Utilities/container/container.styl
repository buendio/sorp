.container {
  padding-right: 20px;
  padding-left: 20px;
  margin-right: auto;
  margin-left: auto;
  width: 100%;
  max-width: 1090px;
}

@media (min-width: screen-xxs-min) {
  .container {
    width: calc(100% - 30px);
  }
}
@media (min-width: screen-xs-min) {
  .container {
    max-width: 1330px;
  }
}