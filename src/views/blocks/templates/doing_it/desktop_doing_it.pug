                img.first-screen__workshop-icon.lazy(data-src="/dist/img/blocks/first-screen/clocks.svg", alt="")
                p.first-screen__workshop-text
                  +lang(
                  'Начало интерактива: ',
                  'Q&A starts: '
                  )
                  span
                    +lang(
                    '10 декабря в 18:00 ч',
                    '10 December at 18:00'
                    )
